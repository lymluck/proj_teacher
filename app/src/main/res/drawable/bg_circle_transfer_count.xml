<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!-- solid指定形状的填充色，只有android:color一个属性 -->
    <solid android:color="#FAFFC4" />
    <!-- padding设置内间距 -->
    <padding
        android:bottom="1dp"
        android:left="6dp"
        android:right="6dp"
        android:top="1dp" />
</shape>